    <div class="maginBod">        

        <div class="container ">
            <div class="row text-left">
                <ol class="breadcrumb">
                    <li class="btn-link"><a href="/">Inicio</a></li>
                    <li class="active">Busqueda</li>
                </o>
            </div>
            <div class="row">
                        <div class="col-md-4 bloq" >
                             <!-- Blog Search Well -->
                            <div class="ol-md-12">
                            <hr>
                                <h4>Buscar trámite o entidad</h4>
                                <!-- buscador -->
                                <div class="col-md-12" >
                                    <div class="col-middle">
                                        <form action="/buscar" method="get" >

                                            <input type="text" name="busqueda" placeholder="Buscar Trámite u Organización" class="" style="color:#000; width:200px" autocomplete="off" required>
                                            <button type="submit" class="btn" style="background-color: #1C1C1C;"><i class="fa fa-search"></i></button>
                                            <hr>
                                        </form>
                                    </div>
                                </div>
                                <!-- /.input-group -->
                                <br>
                            </div>
                        </div>

                        <div class="col-md-8 tab-content">
                            <div class="row breadcrumb">
                                <ul class="nav nav-pills ">
                                    <li role="presentation" class="btn-link bloq">
                                        <a href="#resulBusq1" >( <%= organizaciones.length %>) organizaciones encontradas</a>
                                    </li>
                                    <li role="presentation" class="btn-link bloq">
                                        <a href="#resulBusq2">( <%= tramites.length %>) trámites encontrados</a>
                                    </li>
                                </ul>
                            </div>
                            <div role="tabpanel" class="" id="resulBusq1">
                                
                                <hr>
                                <h5 class="text-left"><u> Resultados de la búsqueda de entidades </u></h5>
                  
                                    <div class="panel-body">
                                        
                                        <!-- Comment -->
                                        <div class="text-left">
                                            <% if(organizaciones.length !== 0){
                                                for(var j=0; j < organizaciones.length; j++){
                                                    var objeto = organizaciones[j];
                                            %>
                                            <div id="searchableProv" class="single_progress_bar">
                                                <span>
                                                    <div>
                                                        <label><u> <a href="<%=objeto.url_view_organizacion%>" class="enlBusq"> <%= objeto.nombre_organizacion %> </a></u> <i class="fa fa-angle-down"></i></label>
                                                         <!--h5><%= objeto.nombre_organizacion %></h5-->
                                                         <br><br>
                                                    </div>
                                                </span>
                                            </div>
                                            <%
                                            }
                                                } else{%>
                                            <h5>Sin resultados</h5>
                                            <% } %>
                                        </div>
                                        <hr>
                                    </div>


                           </div>
                           <br>

                           <div role="tabpanel" class="" id="resulBusq2">
                                
                                <h5 class="text-left"><u> Resultados de la búsqueda de trámites </u></h5>

                                    <div class="panel-body">
                                        
                                        <!-- Comment -->
                                        <div class="text-left">
                                            <% 
                                            if(tramites.length != 0){
                                              for(var j=0; j < tramites.length; j++){
                                                var objeto = tramites[j];
                                            %>
                                            <div id="searchableProv" class="single_progress_bar">
                                                <span>
                                                    <div>
                                                        <b><u><a href="<%=objeto.url_view_tramite%>" class="enlBusq"> <%= objeto.nombre_tramite %> </a></u><i class="fa fa-angle-down"></i></b> 
                                                         <h5><%= objeto.descripcion_tramite%></h5>
                                                    </div>
                                                </span>
                                                <br>
                                            </div>
                                            <%      
                                                    }
                                                } else{%>
                                            <h4>Sin resultados</h4>
                                            <% } %>
                                            
                                        </div>
                                     </div>
                                     <div class="row breadcrumb">
                                        <ul class="nav nav-pills ">
                                            <li role="presentation" class="btn-link bloq">
                                                <a href="#resulBusq1" >( <%= organizaciones.length %>) organizaciones encontradas</a>
                                            </li>
                                            <li role="presentation" class="btn-link bloq">
                                                <a href="#resulBusq2">( <%= tramites.length %>) trámites encontrados</a>
                                            </li>
                                        </ul>
                                    </div>
                                     
                                    <hr>
                            </div>
                            <br>
                        </div>       
            </div>
        </div>
    </div>
    <br><hr>
    