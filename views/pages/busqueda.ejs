    <div class="maginBod">        


        <div class="container ">
            <div class="row">
                        <div class="col-md-4" >
                             <!-- Blog Search Well -->
                            <div class="ol-md-12">
                            <hr>
                                <h4>Buscar trámite o entidad</h4>
                                <!-- buscador -->
                                <div class="col-md-12" >
                                    <div class="col-middle">
                                        <form action="/buscar" method="get" >

                                            <input type="text" name="busqueda" placeholder="Busque Tramite o Entidad" class="" style="color:#000; width:200px" required>
                                            <button type="submit" class="btn"><i class="fa fa-search"></i></button>
                                        </form>
                                    </div>
                                </div>
                                <!-- /.input-group -->
                            </div>
                            <br><hr>
                            <div class="col-md-12">
                                <div class="col-middle">

                                    <h4>Jerarquía organización</h4>

                                        <div class="txtIzq">
                                            <% 
                                               var cantList = 4;
                                               for(var j=0; j < jerarquia_organizacion.length; j++) {
                                                var objeto = jerarquia_organizacion[j];
                                                %>
                                            <div>
                                                <!--h4> <b> <%= objeto.nombre_organizacion %> </b> - </h4-->
                                                <b><a href="<%=objeto.url_view_organizacion%>"> <%= objeto.nombre_organizacion %></a></b>
                                            </div>
                                            <%
                                                }
                                            %>
                                        </div>

                                </div>
                            </div>
                        </div>

                        <div class="col-md-8 tab-content">

                            <div role="tabpanel" class="" id="seite1">
                                
                                        <h1 class="text-muted text-center"><p> Resultados de búsqueda de entidades </p></h1>
                  
                                    <div class="panel-body">
                                        
                                        <!-- Comment -->
                                        <div class="text-left">
                                            <% if(organizacion.length !== 0){
                                                for(var j=0; j < organizacion.length; j++){
                                                    var objeto = organizacion[j];
                                            %>
                                            <div id="searchableProv" class="single_progress_bar">
                                                <span>
                                                    <div>
                                                        <h5> <b> <%= objeto.nombre_organizacion %> </b> - </h5>
                                                        <a href="<%=objeto.url_view_organizacion%>"> <%= objeto.nombre_organizacion %></a>
                                                    </div>
                                                </span>
                                            </div>
                                            <%      if(j === cantList){
                                                            break;
                                                        }
                                                    }
                                                } else{%>
                                            <h4>Sin resultados</h4>
                                            <% } %>
                                        </div>
                                     </div>


                           </div>
                           <br>
                           <div role="tabpanel" class="" id="seite1">
                                
                                <h1 class="text-muted text-center"><p> Resultados de búsqueda de trámites </p></h1>

                                    <div class="panel-body">
                                        
                                        <!-- Comment -->
                                        <div class="text-left">
                                            <% 
                                            if(tramite.length != 0){
                                              for(var j=0; j < tramite.length; j++){
                                                var objeto = tramite[j];
                                            %>
                                            <div id="searchableProv" class="single_progress_bar">
                                                <span>
                                                    <div>
                                                        <h5> <b> <%= objeto.nombre_tramite %> </b> - </h5>
                                                        <a href="<%=objeto.url_view_tramite%>"> <%= objeto.codigo_tramite_ge %></a>
                                                    </div>
                                                </span>
                                            </div>
                                            <%      if(j === cantList){
                                                            break;
                                                        }
                                                    }
                                                } else{%>
                                            <h4>Sin resultados</h4>
                                            <% } %>
                                            
                                        </div>
                                     </div>
                            </div>
                            <br>
                        </div>       
            </div>
        </div>
    </div>
    <br><hr>
    