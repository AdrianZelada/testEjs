<div class="maginBod">        

        <div class="container ">
            <div class="row text-left">
                <ol class="breadcrumb fondoBlanco">
                    <li class="btn-link"><a href="/">Inicio</a></li>
                    <li class="btn-link"><a href="/tramites">Trámites</a></li>
                    <li class="active"><%= tramites.nombre_tramite %></li>
                </o>
            </div>
            <div class="row">
                        <div class="col-md-4 bloq" >
                            <br>
                            <!-- Blog Search Well -->
                            <div class="ol-md-12">
                           
                                <h4>Buscar trámite o entidad</h4>
                                <!-- buscador -->
                                <div class="col-md-12" >
                                    <div class="col-middle">
                                        <form action="/buscar" method="get" >

                                            <input type="text" name="busqueda" placeholder="Buscar Trámite u Organización" class="" style="color:#000; width:200px" autocomplete="off" required>
                                            <button type="submit" class="btn" style="background-color: #1C1C1C;"><i class="fa fa-search"></i></button>
                                        </form>
                                    </div>
                                </div>
                                <br><br>
                                <div class="col-md-12 bloq">
                                    <hr>
                                    <div class="form-group col-md-12"> 
                                        <!--label class="col-md-2">Institución: </label-->
                                                        
                                            <h5><u><a href="<%=tramites.organizacion.url_view_organizacion%>" class="enlBusq"><%= tramites.organizacion.nombre_organizacion %></a></u></h5>
                                        
                                    </div>
                                    <hr>
                                    <hr>
                                
                                    <hr>
                                    <% if(tramites.organizacion.mision !== null ){ %>
                                        <div class="form-group col-md-12"> 
                                            <p class="col-md-2 ">Misión: </p>
                                            <div class="col-md-10 text-left">                 
                                                <h6><%= tramites.organizacion.mision %></h6>
                                            </div>
                                        </div>
                                    <hr>
                                    <%}%>
                               
                                    <hr>
                                    <% if(tramites.organizacion.vision !== null ){ %>
                                    <hr>
                                    <div class="form-group col-md-12"> 
                                        <p class="col-md-2 ">Visión: </p>
                                        <div class="col-md-10 text-left">                 
                                            <h6><%= tramites.organizacion.vision %></h6>
                                        </div>
                                        <hr>
                                    </div>
                                    <hr>
                                    <%}%>
                                </div>

                            </div>
                           
                            <br><hr>
                            
                        </div>

                        <div class="col-md-8 tab-content">
                            
                            <div class="txtIzq" >
                                
                                <div class="form-group col-md-12">                 
                                    <h3><b><%= tramites.nombre_tramite %></b></h3>
                                    <h4 class="text-right"><b><%= tramites.organizacion.nombre_organizacion %></b></h4>
                                </div>
                                <hr>
                                <div class="form-group col-md-12">
                                  
                                  <div class="col-md-12">                     
                                     <%= tramites.descripcion_tramite %>
                                  </div>
                                </div>

                                <hr>
                                
                                
                                <div class="form-group col-md-12">
                                    <label class="col-md-2 control-label">Dirigido A: </label>
                                    <div class="col-md-10">
                                    <% 
                                    for(var j=0; j < tramites.poblacion_objeto.length; j++) {
                                        var objeto = tramites.poblacion_objeto[j];
                                    %>                                    
                                        <%= objeto %> 
                                    <br>
                                    <% } %>
                                    </div>
                                </div>
                               
                                <hr><br>
                                <div class="form-group col-md-12">    
                                    <br>             
                                    <h5><label>Requisitos para realizar el trámite</label></h5>
                                </div>
                                
                                 <div class="form-group col-md-12">
                                    
                                <% 
                                    for(var j=0; j < tramites.requisitos.length; j++) {
                                        var objeto = tramites.requisitos[j];
                                        var papel ="Sin documento";

                                        if(parseInt(objeto.papel_original)==1){ papel ="Documento original"; }
                                        if(parseInt(objeto.papel_original)==1){ papel ="Documento en fotocopia"; }
                                        if(parseInt(objeto.papel_original)==1){ papel ="Documento en fotocopia legalizada"; }
                                        
                                    %>  
                                    
                                        <div class="col-md-12"> 
                                            <% if(objeto.url_view_tramite !== null){ %>
                                                <%= j+1%>.- <u><a href="<%= objeto.url_view_tramite %>" class="enlBusq"><%= objeto.nombre_requisito %></a> <i class="fa fa-caret-down"></i></u><b>( <%= papel%> ). </b>
                                            <% }else{ %>
                                                <%= j+1%>.- <%= objeto.nombre_requisito %> - <b>( <%= papel%> )</b><br><br>
                                            <% } %>
                                        </div>
                                        <hr>
                                        <br>
                                
                                <% }%>
                                </div>
                                
                               
                                <hr><br>
                                <div class="form-group col-md-12">                 
                                    <h5><label>Instrucciones para realizar el trámite</label></h5>
                                </div>
                                <div class="form-group col-md-12">

                                    <% 
                                    for(var j=0; j < tramites.pasos.length; j++) {
                                        var objeto = tramites.pasos[j];
                                    %>                                    
                                    <%= j+1 %>- <%= objeto.descripcion_paso %> 
                                    <br><br>
                                    <% } %>

                                </div>
                           </div>
                           <br>
                            <br>
                        </div>       
            </div>
        </div>
</div>
<br><hr>