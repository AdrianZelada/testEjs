                                    <!--label class="col-md-4 control-label">Sitio web:</label-->
                                    <% 
                                      if(organizacion[0].valor_campo_organizacions.length !== 0){
                                        for(var j=0; j < organizacion[0].valor_campo_organizacions.length; j++) {
                                                
                                    %>    
                                  <div class="col-md-7">            
                                    <%= organizacion[0].valor_campo_organizacions[j].tipo_campo_organizacion.tipo_campo_or %>
                                  </div>
                                  
                                  <%    }
                                      }%>
                                  
                                </div>

                                 <hr>----------
                                
                                
