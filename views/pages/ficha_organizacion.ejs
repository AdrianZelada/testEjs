<div class="maginBod">        

        <div class="container ">
            <div class="text-left">
                <ol class="breadcrumb">
                    <li><a href="/">Inicio</a></li>
                    <li><a href="/organizaciones">Organizaciones</a></li>
                    <li class="active">Ficha organización</li>
                </o>
            </div>
            <div class="row">
                        <div class="col-md-4" >
                             <!-- Blog Search Well -->
                            <div class="">
                                <hr>
                                <h4>Buscar trámite o organización</h4>
                                <!-- buscador -->
                                <div class="col-md-12" >
                                    <div class="col-middle">
                                        <form action="/buscar" method="get" >

                                            <input type="text" name="busqueda" placeholder="Buscar Trámite u Organización" class="" style="color:#000; width:200px" autocomplete="off" required>
                                            <button type="submit" class="btn"><i class="fa fa-search"></i></button>
                                        </form>
                                    </div>
                                </div>
                                <hr>
                                
                            </div>
                            <br>    
                            <div class="col-md-12">
                                <div class="col-middle breadcrumb bloq">
                                    <hr>
                                    <h4 >Trámites ofrecidos </h4>
                                    <p> Esta organización no cuenta con trámites por el momento</p>
                                    <!-- aca va el codigo de tramites ofrecidos -->
                                    <hr>
                                </div>
                            </div>
                            <hr>
                            <div class="col-md-12 bloq">
                                <div class="col-middle">
                                    <hr>
                                    <h4>Esta organización depende de:</h4>

                                        <div class="txtIzq">
                                            <% 
                                               for(var j=0; j < jerarquia.length; j++) {
                                                var objeto = jerarquia[j];
                                                %>
                                            <div>
                                                <b><a href="<%=objeto.url_view_organizacion%>"> <%= j+1%>.- <%= objeto.nombre_organizacion %></a></b>
                                            </div>
                                            <%
                                                }
                                            %>
                                        </div>
                                    <hr>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-8 tab-content">
                            <hr>
                            <div class="txtIzq">
                                <div class="form-group col-md-12 text-center">
                                    <% if(organizacion.logo !== null){ %>
                                        <img src="../../images/<%=organizacion.logo %>" alt="" width="100" height="120">
                                    <% }else{ %>
                                        <img src="../../images/sinImagen.jpg" alt="" width="100" height="120">
                                    <% } %>
                                </div>

                                <div class="form-group col-md-12">
                                    <div class="col-md-12 text-center">                     
                                        <p><%= organizacion.nombre_organizacion %></p>
                                    </div>
                                </div>

                                <% if(organizacion.mision !== null){ %>
                                <hr>
                                <div class="form-group col-md-12">
                                  <label class="col-md-4 control-label">Misión:</label>
                                  <div class="col-md-7">                     
                                    <%= organizacion.mision %>
                                  </div>
                                </div>
                                <% } %>

                                <% if(organizacion.autoridad !== null){ %>
                               
                                <div class="form-group col-md-12">
                                  <label class="col-md-4 control-label">M.A.E.:</label>
                                  <div class="col-md-7">                     
                                    <%= organizacion.autoridad %>
                                  </div>
                                </div>
                                <% } %>
                               
                                    
                                <% 
                                    if(organizacion.campo_organizacion.length !== 0){
                                        for(var j=0; j < organizacion.campo_organizacion.length; j++) {
                                             var objeto = organizacion.campo_organizacion[j];   
                                %> 
                                <div class="form-group col-md-12 corregirTxt">
                                    <label class="col-md-4 control-label"><%= objeto.nombre_campo.replace(/_/," ") %>: </label>   
                                    <div class="col-md-7">            
                                        <%= objeto.valor_campo %>
                                    </div>
                                </div>
                                <%  }
                                  }%>
                                
                                <% if(organizacion.tipo_organizacion.nombre_tipo_organizacion !== null){ %>
                                <div class="form-group col-md-12">
                                  <label class="col-md-4 control-label">Tipo de organización:</label>
                                  <div class="col-md-7">                     
                                    <%= organizacion.tipo_organizacion.nombre_tipo_organizacion %>
                                  </div>
                                </div>
                                <% } %>
                                

                           </div>
                           <br><br>
                        </div>       
            </div>
        </div>
</div>
<br><hr>