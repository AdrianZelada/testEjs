<div class="maginBod">        

        <div class="container ">
            <div class="text-left">
                <ol class="breadcrumb fondoBlanco">
                    <li class="btn-link"><a href="/">Inicio</a></li>
                    <li class="btn-link"><a href="/organizaciones">Organizaciones</a></li>
                    <li class="active"><%= organizacion.nombre_organizacion %></li>
                </o>
            </div>
            <div class="row">
                        <div class="col-md-4 bloq" >
                             <!-- Blog Search Well -->
                            <div class="">
                                <hr>
                                <h4>Buscar trámite o organización</h4>
                                <!-- buscador -->
                                <div class="col-md-12" >
                                    <div class="col-middle">
                                        <form action="/buscar" method="get" >

                                            <input type="text" name="busqueda" placeholder="Buscar Trámite u Organización" class="" style="color:#000; width:200px" autocomplete="off" required>
                                            <button type="submit" class="btn" style="background-color: #1C1C1C;"><i class="fa fa-search"></i></button>
                                        </form>
                                    </div>
                                </div>                               
                            </div>
                            <br>    

                            <% tramitesOfrecidos = organizacion.tramites;
                                if(tramitesOfrecidos.length !== 0){
                            %>
                            <div class="col-md-12">
                                <div class="col-middle">
                                    <hr>
                                    <h4 >Trámites ofrecidos </h4>
                                    
                                    <div class="text-left contenEntidOrgan">
                                        <%
                                                for(var j=0; j < tramitesOfrecidos.length; j++){
                                                    var objeto = tramitesOfrecidos[j];
                                        %>
                                        <div id="searchableProv" class="txtIzq">
                                            <span>
                                                <div>
                                                    <h5> <i class="fa fa-file-text-o"></i>  &nbsp;  <a href="<%=objeto.url_view_tramite%>"> <%= objeto.nombre_tramite %> </a> </h5>
                                                        
                                                </div>
                                            </span>
                                        </div>
                                       <% }  %>
                                    </div>
                                </div>
                            </div>
                            <hr>
                            <% }  %>
                            <div class="col-md-12 bloq">
                                <div class="col-middle">
                                    <hr>
                                    <h4>Dependecia:</h4>

                                        <div class="txtIzq">
                                            <% 
                                               for(var j=0; j < jerarquia.length; j++) {
                                                var objeto = jerarquia[j];
                                                %>
                                            <div>
                                                <b><a href="<%=objeto.url_view_organizacion%>"> <i class="fa fa-angle-double-down"></i>  &nbsp; <%= objeto.nombre_organizacion %></a></b>
                                            </div>
                                            <%
                                                }
                                            %>
                                        </div>
                                    <hr>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-8 tab-content">

                            <div class="txtIzq">
                                <div class="form-group col-md-12 text-center">
                                    <% if(organizacion.logo !== null){ %>
                                        <img src="../../images/<%=organizacion.logo %>" alt="" width="100" height="120">
                                    <% }else{ %>
                                        <img src="../../images/sinImagen.jpg" alt="" width="100" height="120">
                                    <% } %>
                                </div>

                                <div class="form-group col-md-12">
                                    <div class="col-md-12 text-center">                     
                                        <h3><b><%= organizacion.nombre_organizacion %></br></h3>
                                    </div>
                                </div>

                                <% if(organizacion.mision !== null){ %>
                                <hr>
                                <div class="form-group col-md-12">
                                  <b class="col-md-4">Misión:</b>
                                  <div class="col-md-7">                     
                                    <%= organizacion.mision %>
                                  </div>
                                </div>
                                <% } %>
                                <% if(organizacion.vision !== null){ %>
                                <hr>
                                <div class="form-group col-md-12">
                                  <b class="col-md-4">Visión:</b>
                                  <div class="col-md-7">                     
                                    <%= organizacion.vision %>
                                  </div>
                                </div>
                                <% } %>

                                <% if(organizacion.autoridad !== null){ %>
                               
                                <div class="form-group col-md-12">
                                  <b class="col-md-4">Máxima Autoridad Ejecutiva:</b>
                                  <div class="col-md-7">                     
                                    <%= organizacion.autoridad %>
                                  </div>
                                </div>
                                <% } %>
                               
                                    
                                <% 
                                    if(organizacion.campo_organizacion.length !== 0){
                                        for(var j=0; j < organizacion.campo_organizacion.length; j++) {
                                            var objeto = organizacion.campo_organizacion[j];   
                                            var cadena = objeto.nombre_campo.replace(/_/," ");
                                            if(/telefono/.test(cadena)){ cadena = cadena.replace("telefono", "Teléfono"); }
                                            if(/direccion/.test(cadena)){ cadena = cadena.replace("direccion", "Dirección"); }
                                            if(/pagina/.test(cadena)){ cadena = cadena.replace("pagina", "Página"); }
                                             
                                %> 
                                <div class="form-group col-md-12 corregirTxt">
                                    <b class="col-md-4"><%= cadena %>: </b>   
                                    <div class="col-md-7">            
                                        <%= objeto.valor_campo %>
                                    </div>
                                </div>
                                <%  }
                                  }%>
                                
                                <% if(organizacion.tipo_organizacion.nombre_tipo_organizacion !== null){ %>
                                <div class="form-group col-md-12">
                                  <b class="col-md-4">Tipo de organización:</b>
                                  <div class="col-md-7">                     
                                    <%= organizacion.tipo_organizacion.nombre_tipo_organizacion %>
                                  </div>
                                </div>
                                <% } %>
                                

                           </div>
                           <br><br>
                        </div>       
            </div>
        </div>
</div>
<br><hr>

